<div class="row">
  <div class="col-md-8">
    <h1>All Wikis</h1>
  </div>
  <div class="col-md-4">
    <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
  </div>
</div>
<hr>
  <% if @wikis.count > 0 %>
    <%= render @wikis %>
  <% else %>
    <h4>There are no wikis yet.</h4>
  <% end %>
